    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link
        href="https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600;700;800&family=Hind+Siliguri:wght@300;400;500;600;700&display=swap"
        rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css">
    <style>
        :root {
            --primary-color: #0f172a;
            --accent-color: #22c55e;
            --accent-hover: #16a34a;
            --bg-color: #f8fafc;
            --card-bg: #ffffff;
            --border-color: #e2e8f0;
            --text-main: #1e293b;
            --text-muted: #64748b;
            --radius-lg: 16px;
            --radius-md: 12px;
            --shadow: 0 4px 6px -1px rgb(0 0 0 / 0.1), 0 2px 4px -2px rgb(0 0 0 / 0.1);
            --font-family: 'Inter', system-ui, -apple-system, sans-serif;
        }

        * {
            box-sizing: border-box;
            margin: 0;
            padding: 0;
        }

        body {
            font-family: var(--font-family);
            background-color: var(--bg-color);
            color: var(--text-main);
            line-height: 1.5;
            padding-bottom: 40px;
        }

        .top-logo {
            display: flex;
            justify-content: center;
            padding: 20px 0;
        }

        .top-logo img {
            height: 60px;
            width: auto;
            object-fit: contain;
        }

        .container {
            max-width: 800px;
            margin: 0 auto;
            background: var(--card-bg);
            border-radius: var(--radius-lg);
            padding: 40px;
            box-shadow: var(--shadow);
        }

        .header-text {
            text-align: center;
            margin-bottom: 0px;
        }

        .header-text h1 {
            font-size: 32px;
            font-weight: 800;
            text-transform: uppercase;
            letter-spacing: 0.04em;
            margin-bottom: 24px;
        }

        .branding {
            display: flex;
            align-items: center;
            justify-content: center;
            margin-bottom: 0px;
        }

        .branding span.company {
            font-size: 20px;
            font-weight: 700;
            letter-spacing: 0.5px;
        }

        .branding span.tld {
            background: var(--accent-color);
            color: white;
            padding: 0;
            border-radius: 6px;
            font-size: 18px;
            margin-left: 4px;
            font-weight: 700;
            height: 32px;
            width: 56px;
            /* Fixed width to prevent layout jump */
            overflow: hidden;
            display: inline-block;
            vertical-align: middle;
            position: relative;
        }

        .tld-wrapper {
            display: flex;
            flex-direction: row;
            width: 200%;
            height: 100%;
            animation: slideTldHorizontal 4s cubic-bezier(0.8, 0, 0.2, 1) infinite;
        }

        .tld-item {
            width: 50%;
            height: 100%;
            display: flex;
            align-items: center;
            justify-content: center;
            flex-shrink: 0;
        }

        @keyframes slideTldHorizontal {

            0%,
            40% {
                transform: translateX(0);
            }

            50%,
            90% {
                transform: translateX(-50%);
            }

            100% {
                transform: translateX(0);
            }
        }

        .used-message-box {
            text-align: center;
            padding: 40px 20px;
            background: #fff;
            border-radius: var(--radius-md);
            margin-bottom: 30px;
        }

        .warning-icon {
            font-size: 48px;
            color: #f59e0b;
            margin-bottom: 20px;
        }

        .used-title {
            font-size: 24px;
            font-weight: 800;
            color: var(--primary-color);
            margin-bottom: 12px;
        }

        .used-text {
            color: var(--text-muted);
            font-size: 15px;
            line-height: 1.6;
            margin-bottom: 24px;
        }

        .order-qr-btn {
            display: inline-flex;
            align-items: center;
            justify-content: center;
            gap: 20px;
            padding: 12px 40px;
            border: 2px solid #4285f4;
            color: #4285f4;
            text-decoration: none;
            border-radius: 60px;
            font-weight: 800;
            text-transform: uppercase;
            font-size: 15px;
            line-height: 1.3;
            transition: all 0.2s ease;
            max-width: 100%;
        }

        .order-qr-btn:hover {
            background: rgba(66, 133, 244, 0.05);
            border-color: #3367d6;
            color: #3367d6;
        }

        .order-qr-btn .btn-text {
            text-align: center;
        }

        .order-qr-btn i {
            font-size: 24px;
        }

        /* Video Embed Styles */
        .video-section {
            margin-top: 30px;
            width: 100%;
            border-radius: var(--radius-lg);
            overflow: hidden;
            aspect-ratio: 16/9;
            box-shadow: var(--shadow);
        }

        .video-section iframe {
            width: 100%;
            height: 100%;
        }

        .app-buttons {
            display: grid;
            grid-template-columns: 1fr 1fr;
            gap: 20px;
            margin-top: 40px;
        }

        .app-btn {
            background: #e11d48;
            color: white;
            padding: 16px;
            border-radius: 24px;
            text-decoration: none;
            display: flex;
            align-items: center;
            justify-content: center;
            gap: 10px;
            font-size: 14px;
            font-weight: 700;
            text-align: center;
            box-shadow: 0 4px 15px rgba(225, 29, 72, 0.3);
        }

        /* Recent Work Gallery Styles */
        .recent-work-section {
            margin-top: 40px;
            padding-top: 20px;
            border-top: 1px solid var(--border-color);
        }

        .recent-work-title {
            text-align: center;
            margin-bottom: 24px;
        }

        .recent-work-title h3 {
            font-size: 16px;
            font-weight: 500;
            color: var(--primary-color);
            letter-spacing: 0.02em;
            margin-bottom: 4px;
            text-transform: uppercase;
        }

        .work-gallery {
            display: grid;
            grid-template-columns: 1fr 1fr;
            gap: 20px;
        }

        .work-item {
            border-radius: var(--radius-lg);
            overflow: hidden;
            box-shadow: var(--shadow);
            aspect-ratio: 4/3;
            background: #eee;
        }

        .work-item img {
            width: 100%;
            height: 100%;
            object-fit: cover;
            transition: transform 0.3s ease;
        }

        .work-item:hover img {
            transform: scale(1.05);
        }

        @media (max-width: 600px) {
            .work-gallery {
                grid-template-columns: 1fr;
            }
        }

        .footer {
            text-align: center;
            margin-top: 60px;
            padding: 20px;
        }

        .footer p {
            font-size: 13px;
            color: var(--text-muted);
            margin-bottom: 20px;
        }

        .footer-links {
            display: flex;
            justify-content: center;
            gap: 15px;
            font-size: 12px;
            color: var(--text-main);
            margin-bottom: 20px;
            align-items: center;
        }

        .footer-links a {
            color: inherit;
            text-decoration: none;
        }

        #google_translate_element {
            min-height: 40px;
        }

        .goog-te-gadget {
            font-family: inherit !important;
            font-size: 14px !important;
            color: var(--text-muted) !important;
        }

        .goog-te-gadget-simple {
            background-color: white !important;
            border: 1px solid var(--border-color) !important;
            padding: 6px 12px !important;
            border-radius: 8px !important;
            cursor: pointer !important;
            display: inline-flex !important;
            align-items: center !important;
        }

        .goog-te-menu-value span {
            color: var(--text-main) !important;
            font-weight: 500 !important;
        }

        .footer-logo {
            display: flex;
            flex-direction: column;
            align-items: center;
            gap: 12px;
        }

        @media (max-width: 600px) {
            .app-buttons {
                grid-template-columns: 1fr;
            }

            .container {
                padding: 20px;
            }

            .header-text h1 {
                font-size: 24px;
            }

            .order-qr-btn br {
                display: none;
            }
        }
    </style>
</head>

